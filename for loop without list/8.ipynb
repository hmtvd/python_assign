   "source": [
    "a=int(input(\"enter the +ve number\"))\n",
    "div_lst1=[]\n",
    "for i in range(1,a//2):                    # using for loop\n",
    "    if a%i==0:\n",
    "        div_lst1.append(i)\n",
    "div_lst1.append(a)\n",
    "print(div_lst1)"
